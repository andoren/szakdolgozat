<Window x:Class="IktatogRPCClient.Views.PopUpView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IktatogRPCClient.Views"
        mc:Ignorable="d"
        AllowsTransparency="True"
        FontFamily="Robot"
        FontSize="22"
        SnapsToDevicePixels="True"
        WindowStyle="None"  WindowStartupLocation="CenterOwner" ResizeMode="NoResize" SizeToContent="WidthAndHeight" >
    <Grid VerticalAlignment="Center" HorizontalAlignment="Center" Background="White">
            <ContentControl HorizontalAlignment="Stretch" VerticalAlignment="Stretch" x:Name="ActiveItem"></ContentControl>
    </Grid>
   
</Window>
